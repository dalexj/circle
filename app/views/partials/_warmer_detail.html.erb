<div class="portfolio-modal modal fade" id="portfolioModal<%= index %>" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-content">
    <div class="close-modal" data-dismiss="modal">
      <div class="lr">
        <div class="rl">
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
          <div class="modal-body">
            <h2><%= warmer.name %></h2>
            <hr class="star-primary">
            <img src="<%= warmer.img_url %>" class='img-responsive img-centered'>
            <p><%= warmer.description %></p>
            <ul class="list-inline item-details">
              <li>
              <strong><a href="<%= warmer.linkedin_url %>">LinkedIn</a>
              </strong>
              </li>
              <li>
              <strong><a href="#">Resume</a>
              </strong>
              </li>
            </ul>
            <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Close</button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
